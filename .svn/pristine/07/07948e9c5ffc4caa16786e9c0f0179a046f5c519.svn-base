package com.ssa.cms.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import java.io.Serializable;

import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "TABLE")
public class DTable implements Serializable {

    @Column(name = "TABLE_COMMENT")
    private String tableComment;

    @Column(name = "CREATE_OPTIONS")
    private String createOptions;

    @Column(name = "CHECKSUM")
    private String checkSum;

    @Column(name = "TABLE_COLLATION")
    private String tableCollation;

    @Column(name = "CHECK_TIME")
    private String checkTime;

    @Column(name = "UPDATE_TIME")
    private String updateTime;

    @Column(name = "CREATE_TIME")
    private String createTime;

    @Column(name = "AUTO_INCREMENT")
    private String autoIncrement;

    @Column(name = "DATA_FREE")
    private String dataFree;

    @Column(name = "INDEX_LENGTH")
    private String indexLength;

    @Column(name = "MAX_DATA_LENGTH")
    private String maxDataLength;

    @Column(name = "DATA_LENGTH")
    private String dataLength;

    @Column(name = "AVG_ROW_LENGTH")
    private String avgRowLength;

    @Column(name = "TABLE_ROWS")
    private String tableRows;

    @Column(name = "ROW_FORMAT")
    private String rowFormat;

    @Column(name = "VERSION")
    private String version;

    @Column(name = "ENGINE")
    private String engine;

    @Column(name = "TABLE_TYPE")
    private String tableType;

    @Id
    @Column(name = "TABLE_NAME")
    private String tableName;

    @Column(name = "TABLE_SCHEMA")
    private String tableSchema;

    public String getTableComment() {
        return tableComment;
    }

    public String getCreateOptions() {
        return createOptions;
    }

    public String getCheckSum() {
        return checkSum;
    }

    public String getTableCollation() {
        return tableCollation;
    }

    public String getCheckTime() {
        return checkTime;
    }

    public String getUpdateTime() {
        return updateTime;
    }

    public String getCreateTime() {
        return createTime;
    }

    public String getAutoIncrement() {
        return autoIncrement;
    }

    public String getDataFree() {
        return dataFree;
    }

    public String getIndexLength() {
        return indexLength;
    }

    public String getMaxDataLength() {
        return maxDataLength;
    }

    public String getDataLength() {
        return dataLength;
    }

    public String getAvgRowLength() {
        return avgRowLength;
    }

    public String getTableRows() {
        return tableRows;
    }

    public String getRowFormat() {
        return rowFormat;
    }

    public String getVersion() {
        return version;
    }

    public String getEngine() {
        return engine;
    }

    public String getTableType() {
        return tableType;
    }

    public String getTableName() {
        return tableName;
    }

    public String getTableSchema() {
        return tableSchema;
    }
}
